{
    "C_Cpp.intelliSenseEngineFallback": "Disabled",
    "C_Cpp.loggingLevel": "Information",
    "git.enabled": false,
    "editor.find.autoFindInSelection": true,
    "workbench.colorTheme": "Monokai",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/*.DS_Store": true,
        "**/*.o": true,
        "**/*.cmd": true,
        "**/*.mod.c": true,
        "**/.tmp_versions": true,
        "**/modules.order": true,
        // ".vscode": true,
        "**/*.ko": true,
        "**/.smbdelete*": true,
        "**/Module.symvers": true,
        "**/._*": true,
        "tempCodeRunnerFile": true,
        "util/mkfs": true,
        "util/debugfs": true,
        "util/fsck": true,
        "a.out": true
    },
    "files.associations": {
        "*.h": "c"
    },
}